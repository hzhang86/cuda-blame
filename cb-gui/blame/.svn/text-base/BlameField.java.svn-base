package blame;

public class BlameField {

	String name;
	int fieldNumber;
	
	public BlameField(String n, int fNum)
	{
		name = n;
		fieldNumber = fNum;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getFieldNumber() {
		return fieldNumber;
	}
	public void setFieldNumber(int fieldNumber) {
		this.fieldNumber = fieldNumber;
	}
	
	
}
